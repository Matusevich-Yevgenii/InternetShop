<Window x:Class="InternetShop.WindowsFolder.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InternetShop.WindowsFolder"
        mc:Ignorable="d"
        Title="RegisterWindow" Height="600" Width="800"  MinWidth="800" MinHeight="600">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="177,164,0,0" TextWrapping="Wrap" Text="{Binding ElementName=slider, Path=Value, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" VerticalAlignment="Top" Width="175"/>
        <Slider x:Name="slider" HorizontalAlignment="Left" Margin="177,202,0,0" VerticalAlignment="Top" Width="175" TickFrequency="1"
                IsSnapToTickEnabled="True" Minimum="0" Maximum="9999999"/>
        <Label x:Name="label" Content="Number telephone" HorizontalAlignment="Left" Margin="62,162,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
